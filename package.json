{
  "name": "mfeplayground",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:shell": "ng serve shell  -o",
    "start:mfe1": "ng serve mfe1 -o",
    "start:mfe2": "ng serve mfe2 -o",
    "build:shell": "ng build shell --prod",
    "build:mfe1": "ng build mfe1 --prod",
    "build:mfe2": "ng build mfe2 --prod",
    "start": "concurrently \"npm run start:shell\" \"npm run start:mfe1\" \"npm run start:mfe2\"",
    "build": "npm run build:shell && npm run build:mfe1 && npm run build:mfe2",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^14.3.10",
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "concurrently": "^7.4.0",
    "mfe-library": "file:dist/mfe-library",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.5",
    "@angular/cli": "~14.2.5",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.2.0",
    "ngx-build-plus": "^14.0.0",
    "typescript": "~4.7.2"
  }
}
